{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "RiyAssignADF"
		},
		"FromAdls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'FromAdls'"
		},
		"LS_ToAdls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ToAdls'"
		},
		"LS_ToNotebook_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_ToNotebook'"
		},
		"FromAdls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://riyassignadls.dfs.core.windows.net/"
		},
		"LS_ToAdls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://riyassignadls.dfs.core.windows.net/"
		},
		"LS_from_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat('https://api.openaq.org/v3/sensors/791/days')}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RiyAssignADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FromAdls_accountKey": {
						"value": "[parameters('FromAdls_accountKey')]"
					},
					"LS_ToAdls_accountKey": {
						"value": "[parameters('LS_ToAdls_accountKey')]"
					},
					"LS_ToNotebook_accessToken": {
						"value": "[parameters('LS_ToNotebook_accessToken')]"
					},
					"FromAdls_properties_typeProperties_url": {
						"value": "[parameters('FromAdls_properties_typeProperties_url')]"
					},
					"LS_ToAdls_properties_typeProperties_url": {
						"value": "[parameters('LS_ToAdls_properties_typeProperties_url')]"
					},
					"LS_from_api_properties_typeProperties_url": {
						"value": "[parameters('LS_from_api_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}