{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "RiyAssignADF"
		},
		"FromAdls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'FromAdls'"
		},
		"LS_CsvToAdls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_CsvToAdls'"
		},
		"LS_S3Excle_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'LS_S3Excle'"
		},
		"LS_SqlDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SqlDb'"
		},
		"LS_ToAdls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ToAdls'"
		},
		"LS_ToNotebook_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_ToNotebook'"
		},
		"FromAdls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://riyassignadls.dfs.core.windows.net/"
		},
		"LS_S3Excle_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAQEIP3UWSXRW3ER6D"
		},
		"LS_SqlDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "riyaldbsrvr.database.windows.net"
		},
		"LS_SqlDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbAssignment1"
		},
		"LS_SqlDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "radmin"
		},
		"LS_ToAdls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://riyassignadls.dfs.core.windows.net/"
		},
		"LS_from_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat('https://api.openaq.org/v3/sensors/',string(linkedService().Sensorid),'/days')}"
		},
		"StorageAccountEvent_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b2c51e8b-c017-4c6a-bd57-27706b269aac/resourceGroups/RiyAssignOne/providers/Microsoft.Storage/storageAccounts/riyassignadls"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RiyAssignADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FromAdls_accountKey": {
						"value": "[parameters('FromAdls_accountKey')]"
					},
					"LS_CsvToAdls_connectionString": {
						"value": "[parameters('LS_CsvToAdls_connectionString')]"
					},
					"LS_S3Excle_secretAccessKey": {
						"value": "[parameters('LS_S3Excle_secretAccessKey')]"
					},
					"LS_SqlDb_password": {
						"value": "[parameters('LS_SqlDb_password')]"
					},
					"LS_ToAdls_accountKey": {
						"value": "[parameters('LS_ToAdls_accountKey')]"
					},
					"LS_ToNotebook_accessToken": {
						"value": "[parameters('LS_ToNotebook_accessToken')]"
					},
					"FromAdls_properties_typeProperties_url": {
						"value": "[parameters('FromAdls_properties_typeProperties_url')]"
					},
					"LS_S3Excle_properties_typeProperties_accessKeyId": {
						"value": "[parameters('LS_S3Excle_properties_typeProperties_accessKeyId')]"
					},
					"LS_SqlDb_properties_typeProperties_server": {
						"value": "[parameters('LS_SqlDb_properties_typeProperties_server')]"
					},
					"LS_SqlDb_properties_typeProperties_database": {
						"value": "[parameters('LS_SqlDb_properties_typeProperties_database')]"
					},
					"LS_SqlDb_properties_typeProperties_userName": {
						"value": "[parameters('LS_SqlDb_properties_typeProperties_userName')]"
					},
					"LS_ToAdls_properties_typeProperties_url": {
						"value": "[parameters('LS_ToAdls_properties_typeProperties_url')]"
					},
					"LS_from_api_properties_typeProperties_url": {
						"value": "[parameters('LS_from_api_properties_typeProperties_url')]"
					},
					"StorageAccountEvent_properties_typeProperties_scope": {
						"value": "[parameters('StorageAccountEvent_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}